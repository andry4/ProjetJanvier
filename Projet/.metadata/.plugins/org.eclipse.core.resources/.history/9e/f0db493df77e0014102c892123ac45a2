/**
 * Une classe pour caractérisé la chambre
 */
package projet;

/**
 * @author Andry, François, Christophe
 *
 */

public class Chambre extends Etage {
	public static final float PRIXACHAT = 50000;
	public static final float PRIXVENTE = 35000;
	public static final float RECETTEMAX = 5000;
	public static final float DEPENSE = 1000;
	
	public Chambre(){
		this.nom="Chambre";
		niveauEtage ++;
		nbr++;
	}
}
