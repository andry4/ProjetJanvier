/**
 * Une classe pour caractérisé la chambre
 */
package projet;

/**
 * @author Andry, François, Christophe
 *
 */

public class Chambre extends Etage {

	public Chambre(){
		this.nom="Chambre";
		niveauEtage ++;
		nbrChambre ++;
		setPRIXACHAT(50000);
		PRIXVENTE = 35000;
		RECETTEMAX = 5000;
		DEPENSE = 1000;
	}
}
