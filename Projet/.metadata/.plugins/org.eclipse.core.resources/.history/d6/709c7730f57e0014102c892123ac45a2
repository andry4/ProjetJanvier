/**
 * Une classe pur spécifier chaque étage
 */
package projet;

/**
 * @author Andry, François, Christophe
 *
 */

public class Etage extends Hotel{
	protected String nom;
	public static int niveauEtage = -1;
	
	public Etage(){
		this.nom="inconnue";
		niveauEtage ++;
	}
	public Etage(String name){
		this.nom=name;
		niveauEtage ++;
	}
}
