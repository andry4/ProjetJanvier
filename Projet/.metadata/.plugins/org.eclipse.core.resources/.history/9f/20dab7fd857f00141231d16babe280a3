/**
 * Une classe pour caractérisé le bar
 */
package projet;

/**
 * @author Andry, François, Christophe
 *
 */

public class Bar extends Etage {
	protected static int nbr = 0;
	protected static float PRIXACHAT = 35000;
	protected static float PRIXVENTE = 25000;
	protected static float RECETTEMAX = 3500;
	protected static float DEPENSE = 500;
	
	public Bar(){
		this.nom="Bar";
		niveauEtage ++;
		nbr++;
	}
	
}
