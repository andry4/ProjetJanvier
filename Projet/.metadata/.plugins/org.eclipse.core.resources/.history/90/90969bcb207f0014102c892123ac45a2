/**
 * Une classe pour caractérisé le réstaurant
 */
package projet;

/**
 * @author Andry, François, Christophe
 *
 */

public class Restaurant extends Etage {
	
	public Restaurant(){
		this.nom="Restaurant";
		niveauEtage ++;
		nbrChambre ++;
		PRIXACHAT = 50000;
		PRIXVENTE = 35000;
		RECETTEMAX = 5000;
		DEPENSE = 2000;
	}
	
}
