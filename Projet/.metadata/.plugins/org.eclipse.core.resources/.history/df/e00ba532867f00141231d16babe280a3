/**
 * Une classe pour caractérisé le réstaurant
 */
package projet;

/**
 * @author Andry, François, Christophe
 *
 */

public class Restaurant extends Etage {
	protected static int nbr = 0;
	protected static float PRIXACHAT = 50000;
	protected static float PRIXVENTE = 35000;
	protected static float RECETTEMAX = 5000;
	protected static float DEPENSE = 2000;
	
	public Restaurant(){
		this.nom="Restaurant";
		niveauEtage ++;
		nbr++;
	}
	
}
