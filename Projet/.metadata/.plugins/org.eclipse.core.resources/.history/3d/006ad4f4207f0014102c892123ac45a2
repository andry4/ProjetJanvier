/**
 * Une classe pour caractérisé le salle de sport
 */
package projet;

/**
 * @author Andry, François, Christophe
 *
 */

public class SalleSport extends Etage {
	protected static int nbr = 0;
	protected static float PRIXACHAT = 10000;
	protected static float PRIXVENTE = 7500;
	protected static float RECETTEMAX = 1000;
	protected static float DEPENSE = 100;
	
	public SalleSport(){
		this.nom="Salle de sport";
		niveauEtage ++;
		nbrChambre ++;
		PRIXACHAT = 10000;
		PRIXVENTE = 7500;
		RECETTEMAX = 1000;
		DEPENSE = 100;
	}
	
}
